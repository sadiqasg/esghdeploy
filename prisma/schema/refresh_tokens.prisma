model RefreshToken {
  id            Int      @id @default(autoincrement())
  user_id       Int
  user          User     @relation(fields: [user_id], references: [id])
  refresh_token String   @unique
  expires_at    DateTime
  created_at    DateTime @default(now())

  @@index([user_id])
  @@index([refresh_token])
  @@map("refresh_tokens")
}
